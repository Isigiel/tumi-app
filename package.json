{
  "name": "tumi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "build:stats": "ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy": "yarn run build:prod",
    "deploy": "firebase deploy",
    "precommit": "lint-staged",
    "prettify": "prettier --write ./**/*.{json,css,scss,md,js,ts}",
    "prebundle-report": "yarn run build:stats",
    "prebundle-report:next": "yarn run build:stats",
    "bundle-report": "webpack-bundle-analyzer dist/tumi/stats-es5.json",
    "bundle-report:next": "webpack-bundle-analyzer dist/tumi/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular-buddies/prettier": "^1.0.0-alpha.3",
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.1.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.1.4",
    "@angular/material-moment-adapter": "^8.1.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/pwa": "^0.803.1",
    "@angular/router": "~8.2.4",
    "@angular/service-worker": "~8.2.4",
    "firebase": "6.4.2",
    "first-input-delay": "^0.1.3",
    "hammerjs": "^2.0.8",
    "jspdf": "^1.5.3",
    "moment": "^2.24.0",
    "qrcode": "^1.4.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.803.1",
    "@angular-devkit/build-angular": "~0.803.1",
    "@angular/cli": "~8.3.1",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/luxon": "^1.15.2",
    "@types/node": "~12.7.2",
    "codelyzer": "^5.0.0",
    "firebase-tools": "^7.3.0",
    "fuzzy": "^0.1.3",
    "husky": "^3.0.4",
    "inquirer": "^6.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.12.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "typescript": "<3.6.0",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
