{
  "name": "tumi-app",
  "version": "2.0.0-deploy.15",
  "private": true,
  "scripts": {
    "build:all": "yarn run build:ssr && yarn run copy:hosting",
    "build:functions": "npm run --prefix functions build",
    "build:prod": "ng build --prod",
    "build:ssr": "ng build --prod --source-map && ng run tumi-app:server:production",
    "copy:hosting": "copyfiles dist/**/* functions",
    "deploy": "ng deploy",
    "deploy:all": "firebase deploy",
    "deploy:functions": "yarn run build:all && pm run --prefix functions deploy",
    "dev:ssr": "ng run tumi-app:serve-ssr",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "postdeploy:all": "cross-var sentry-cli releases files \"tumi-app@$npm_package_version\" upload-sourcemaps ./dist/tumi-app/browser && cross-var sentry-cli releases deploys \"tumi-app@$npm_package_version\" new -e production && cross-var sentry-cli releases finalize \"tumi-app@$npm_package_version\"&& cross-var sentry-cli releases deploys \"universal-render@$npm_package_version\" new -e production && cross-var sentry-cli releases finalize \"universal-render@$npm_package_version\"",
    "postrelease:deploy": "yarn run sentry:release:start",
    "prebuild:all": "yarn run release:deploy",
    "precopy:hosting": "rimraf functions/dist",
    "predeploy:all": "yarn run build:all",
    "prerender": "ng run tumi-app:prerender",
    "release": "standard-version --no-verify",
    "release:deploy": "standard-version --prerelease deploy --no-verify --skip.changelog --skip.tag --skip.commit",
    "release:preview": "standard-version --no-verify --dry-run",
    "sentry:release:start": "cross-var sentry-cli releases new \"tumi-app@$npm_package_version\" && cross-var sentry-cli releases new \"universal-render@$npm_package_version\"",
    "serve:ssr": "node dist/tumi-app/server/main.js",
    "start": "ng serve",
    "statistics": "ng build --prod --stats-json && webpack-bundle-analyzer dist/tumi-app/browser/stats.json",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/platform-server": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@angular/service-worker": "^11.2.6",
    "@ngrx/component": "^11.0.0-rc.0",
    "@nguniversal/express-engine": "^11.2.1",
    "@sentry/angular": "^6.2.3",
    "@sentry/node": "^6.2.3",
    "@sentry/tracing": "^6.2.3",
    "algoliasearch": "^4.8.6",
    "bufferutil": "^4.0.3",
    "date-fns": "^2.19.0",
    "express": "^4.15.2",
    "firebase": "8.3.1",
    "lodash-es": "^4.17.21",
    "ngx-markdown": "^11.1.2",
    "ngx-paypal": "^7.0.0",
    "qrcode": "^1.4.4",
    "rxjs": "~6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1102.5",
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular-eslint/builder": "^2.0.2",
    "@angular-eslint/eslint-plugin": "^2.0.2",
    "@angular-eslint/eslint-plugin-template": "^2.0.2",
    "@angular-eslint/schematics": "^2.0.2",
    "@angular-eslint/template-parser": "^2.0.2",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@nguniversal/builders": "^11.2.1",
    "@sentry/cli": "^1.63.1",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.11.1",
    "@types/parse5": "^6.0.0",
    "@types/qrcode": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "copyfiles": "^2.4.1",
    "cross-var": "^1.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "firebase-tools": "^9.6.1",
    "fs-jetpack": "^4.1.0",
    "fuzzy": "^0.1.3",
    "husky": "^5.2.0",
    "inquirer": "^8.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "open": "^8.0.4",
    "postcss": "^8.2.8",
    "prettier": "2.2.1",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-tailwind": "^2.2.10",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.27.2",
    "standard-version": "^9.1.1",
    "tailwindcss": "^2.0.4",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.3",
    "utf-8-validate": "^5.0.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "ws": "^7.4.4",
    "xhr2": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  }
}
