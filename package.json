{
  "name": "tumi",
  "version": "0.0.3-deploy.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "build:staging": "ng build --configuration=staging",
    "build:stats": "ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy": "firebase use default && yarn run release:deploy && yarn run build:prod",
    "predeploy:full": "yarn run predeploy",
    "predeploy:staging": "firebase use staging && yarn run build:staging",
    "deploy:full": "yarn run upload:all",
    "deploy:staging": "yarn run upload:all",
    "postdeploy:staging": "firebase use default",
    "deploy": "yarn run upload",
    "upload": "node upload.js hosting",
    "upload:all": "node upload.js",
    "precommit": "lint-staged",
    "prettify": "prettier --write ./**/*.{json,css,scss,md,js,ts}",
    "prebundle-report": "yarn run build:stats",
    "prebundle-report:next": "yarn run build:stats",
    "bundle-report": "webpack-bundle-analyzer dist/tumi/stats-es5.json",
    "bundle-report:next": "webpack-bundle-analyzer dist/tumi/stats-es2015.json",
    "release": "standard-version --no-verify",
    "release:preview": "standard-version --no-verify --dry-run",
    "release:deploy": "standard-version --prerelease deploy --no-verify --skip.changelog --skip.tag --skip.commit",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "^10.0.2",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/fire": "^6.0.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.4",
    "@angular/localize": "^10.0.4",
    "@angular/material": "^10.0.2",
    "@angular/material-moment-adapter": "^10.0.2",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@angular/service-worker": "~10.0.4",
    "@lottiefiles/lottie-player": "^0.5.1",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/form-plugin": "^3.6.2",
    "@ngxs/router-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "firebase": "^7.15.5",
    "jspdf": "^1.5.3",
    "lodash": "^4.17.17",
    "moment": "^2.27.0",
    "ngx-markdown": "^10.0.0",
    "parallax-js": "^3.1.0",
    "qrcode": "^1.4.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@angular/language-service": "~10.0.4",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "^4.14.152",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "firebase-tools": "^8.4.3",
    "git-last-commit": "^1.0.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "rxjs-spy": "^7.5.2",
    "standard-version": "^8.0.0",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "typescript": "3.9.6",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
