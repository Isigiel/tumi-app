{
  "name": "tumi-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:all": "yarn run build:ssr && yarn run copy:hosting",
    "build:functions": "npm run --prefix functions build",
    "build:prod": "cross-env NODE_ENV=production ng build --prod",
    "build:ssr": "ng build --prod && ng run tumi-app:server:production",
    "copy:hosting": "copyfiles dist/**/* functions",
    "deploy": "ng deploy",
    "deploy:all": "firebase deploy",
    "deploy:functions": "yarn run build:all && pm run --prefix functions deploy",
    "dev:ssr": "ng run tumi-app:serve-ssr",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "prebuild:all": "cross-env NODE_ENV=production",
    "precopy:hosting": "rimraf functions/dist",
    "predeploy:all": "yarn run build:all",
    "prerender": "ng run tumi-app:prerender",
    "prestatistics": "cross-env NODE_ENV=production",
    "serve:ssr": "node dist/tumi-app/server/main.js",
    "start": "ng serve",
    "statistics": "ng build --prod --stats-json && webpack-bundle-analyzer dist/tumi-app/browser/stats.json",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/cdk": "^11.0.2",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.0.4",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/platform-server": "~11.0.4",
    "@angular/router": "~11.0.4",
    "@angular/service-worker": "^11.0.4",
    "@ngrx/component": "^10.1.0",
    "@nguniversal/express-engine": "^11.0.1",
    "bufferutil": "^4.0.2",
    "date-fns": "^2.16.1",
    "express": "^4.15.2",
    "firebase": "^8.2.0",
    "ngx-markdown": "^11.0.1",
    "ngx-paypal": "^6.2.0",
    "qrcode": "^1.4.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "utf-8-validate": "^5.0.3",
    "ws": "^7.4.1",
    "xhr2": "^0.2.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1200",
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular-eslint/builder": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.6",
    "@angular-eslint/schematics": "^0.8.0-beta.5",
    "@angular-eslint/template-parser": "0.8.0-beta.6",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@nguniversal/builders": "^11.0.1",
    "@tailwindcss/typography": "^0.3.1",
    "@types/express": "^4.17.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/parse5": "^5.0.3",
    "@types/qrcode": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "autoprefixer": "^10.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.9",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "firebase-tools": "^9.0.1",
    "fuzzy": "^0.1.3",
    "husky": "^4.3.5",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "ngx-build-plus": "^10.1.1",
    "ngx-tailwind": "^1.1.0",
    "open": "^7.0.3",
    "postcss": "^8.1.7",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-scss": "^3.0.4",
    "prettier": "2.2.1",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-tailwind": "^2.2.8",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,ts}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
