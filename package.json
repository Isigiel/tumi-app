{
  "name": "tumi",
  "version": "0.0.2-deploy.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "build:staging": "ng build --configuration=staging",
    "build:stats": "ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy": "firebase use default && yarn run release:deploy && yarn run build:prod",
    "predeploy:full": "yarn run predeploy",
    "predeploy:staging": "firebase use staging && yarn run build:staging",
    "deploy:full": "yarn run upload:all",
    "deploy:staging": "yarn run upload:all",
    "postdeploy:staging": "firebase use default",
    "deploy": "yarn run upload",
    "upload": "node upload.js hosting",
    "upload:all": "node upload.js",
    "precommit": "lint-staged",
    "prettify": "prettier --write ./**/*.{json,css,scss,md,js,ts}",
    "prebundle-report": "yarn run build:stats",
    "prebundle-report:next": "yarn run build:stats",
    "bundle-report": "webpack-bundle-analyzer dist/tumi/stats-es5.json",
    "bundle-report:next": "webpack-bundle-analyzer dist/tumi/stats-es2015.json",
    "release": "standard-version --no-verify",
    "release:preview": "standard-version --no-verify --dry-run",
    "release:deploy": "standard-version --prerelease deploy --no-verify --skip.changelog --skip.tag --skip.commit",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/material-moment-adapter": "^9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@angular/service-worker": "~9.0.0",
    "@ngxs/devtools-plugin": "^3.6.1",
    "@ngxs/form-plugin": "^3.6.1",
    "@ngxs/router-plugin": "^3.6.1",
    "@ngxs/storage-plugin": "^3.6.1",
    "@ngxs/store": "^3.6.1",
    "@types/lodash": "^4.14.149",
    "firebase": "^7.6.2",
    "jspdf": "^1.5.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ngx-markdown": "^9.0.0-beta.0",
    "parallax-js": "^3.1.0",
    "qrcode": "1.4.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "firebase-tools": "^7.12.1",
    "git-last-commit": "^1.0.0",
    "husky": "^4.0.10",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.3.0",
    "prettier": "^1.12.1",
    "protractor": "~5.4.0",
    "rxjs-spy": "^7.5.1",
    "standard-version": "^7.0.1",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "3.7.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
