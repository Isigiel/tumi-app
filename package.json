{
  "name": "tumi-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:all": "yarn run build:ssr && yarn run copy:hosting",
    "build:functions": "npm run --prefix functions build",
    "build:prod": "ng build --prod",
    "build:ssr": "ng build --prod && ng run tumi-app:server:production",
    "copy:hosting": "copyfiles dist/**/* functions",
    "deploy": "ng deploy",
    "deploy:all": "firebase deploy",
    "deploy:functions": "yarn run build:all && pm run --prefix functions deploy",
    "dev:ssr": "ng run tumi-app:serve-ssr",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "precopy:hosting": "rimraf functions/dist",
    "predeploy:all": "yarn run build:all",
    "prerender": "ng run tumi-app:prerender",
    "serve:ssr": "node dist/tumi-app/server/main.js",
    "start": "ng serve",
    "statistics": "ng build --prod --stats-json && webpack-bundle-analyzer dist/tumi-app/browser/stats.json",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "^11.1.2",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.1.2",
    "@angular/material": "^11.1.2",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/platform-server": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@angular/service-worker": "^11.1.2",
    "@ngrx/component": "^11.0.0-rc.0",
    "@nguniversal/express-engine": "^11.1.2",
    "algoliasearch": "^4.8.4",
    "bufferutil": "^4.0.3",
    "date-fns": "^2.17.0",
    "express": "^4.15.2",
    "firebase": "^8.2.6",
    "lodash-es": "^4.17.20",
    "ngx-markdown": "^11.0.1",
    "ngx-paypal": "^7.0.0",
    "qrcode": "^1.4.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/architect": "0.1102.0-rc.1",
    "@angular-devkit/build-angular": "~0.1102.0-rc.1",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "^1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "~11.2.0-rc.1",
    "@angular/compiler-cli": "~11.1.2",
    "@nguniversal/builders": "^11.1.2",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.11.1",
    "@types/parse5": "^6.0.0",
    "@types/qrcode": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "firebase-tools": "^9.3.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.3.8",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "open": "^7.4.0",
    "postcss": "^8.2.5",
    "prettier": "2.2.1",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-tailwind": "^2.2.8",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.3",
    "utf-8-validate": "^5.0.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "ws": "^7.4.3",
    "xhr2": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  }
}
